{
  "name": "@cygnus/backend-api",
  "version": "1.2.2",
  "description": "API de Cygnus construida con Express y TypeScript.",
  "main": "dist/src/index.js",
  "scripts": {
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "build": "npm run build:app && node ./scripts/update-build-number.mjs",
    "build:app": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/index.ts",
    "start": "node dist/src/index.js",
    "test": "vitest run",
    "test:watch": "vitest watch --run",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Nicol√°s Panaino",
  "buildNumber": "251111674",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "pino": "^9.13.0",
    "pino-http": "^10.5.0",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.8.1",
    "@types/supertest": "^2.0.16",
    "build-number-generator": "^3.0.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "semver": "^7.7.3",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.9",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.45.0",
    "vitest": "^3.2.4"
  }
}
