{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "rootDir": ".",
    "baseUrl": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "paths": {
      "integrations/*": ["integrations/*"],
      "src/*": ["src/*"],
      "app/*": ["app/*"]
    }
  }
}
